{
  "name": "nx-code-mods",
  "version": "1.3.1",
  "description": "Nx generator code modifier toolkit for typescript code using tsquery",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "homepage": "https://github.com/kristianmandrup/nx-code-mods#readme",
  "bugs": {
    "url": "https://github.com/kristianmandrup/nx-code-mods/issues",
    "email": "kmandrup@gmail.com"
  },
  "repository": "github:kristianmandrup/nx-code-mods",
  "scripts": {
    "test": "jest --config jest.config.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "nx",
    "generator",
    "utils",
    "typescript",
    "code",
    "modifications",
    "tools",
    "toolkit",
    "tsquery"
  ],
  "files": [
    "lib/**/*"
  ],
  "author": "Kristian Mandrup <kmandrup@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "noun-json": "^1.0.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3",
    "verb-corpus": "^3.1.5"
  },
  "dependencies": {
    "@nrwl/devkit": "13.9.5",
    "@nrwl/workspace": "13.9.5",
    "@phenomnomnominal/tsquery": "^4.2.0",
    "adjectives": "^1.0.2",
    "humanize-string": "^3.0.0",
    "prepositions": "^0.0.0"
  }
}
