{
  "name": "nx-code-mods",
  "version": "1.0.0",
  "description": "Nx generator code mods using tsquery",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jest.config.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "nx",
    "generators",
    "utils",
    "typescript",
    "code",
    "modifications"
  ],
  "files": [
    "lib/**/*"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@nrwl/devkit": "^13.9.7",
    "@nrwl/workspace": "^13.9.7",
    "@phenomnomnominal/tsquery": "^4.2.0"
  }
}
